	<sql id="Base_Where_Sql_Use">
		<where>
		<if test="alarmPhone != null and alarmPhone != ''">
			and ALARM_PHONE = #{alarmPhone,jdbcType=VARCHAR}
		</if>
		<if test="claimCount != null and claimCount != ''">
			and CLAIM_COUNT = #{claimCount,jdbcType=INTEGER}
		</if>
		<if test="lossFlag != null and lossFlag != ''">
			and LOSS_FLAG = #{lossFlag,jdbcType=CHAR}
		</if>
		<if test="renewalPolicyNo != null and renewalPolicyNo != ''">
			and RENEWAL_POLICY_NO = #{renewalPolicyNo,jdbcType=VARCHAR}
		</if>
		<if test="engineNumber != null and engineNumber != ''">
			and ENGINE_NUMBER = #{engineNumber,jdbcType=VARCHAR}
		</if>
		<if test="vin != null and vin != ''">
			and VIN = #{vin,jdbcType=VARCHAR}
		</if>
		<if test="updateDate != null and updateDate != ''">
			and UPDATE_DATE = #{updateDate,jdbcType=DATE}
		</if>
		<if test="policyHolderPhone != null and policyHolderPhone != ''">
			and POLICY_HOLDER_PHONE = #{policyHolderPhone,jdbcType=VARCHAR}
		</if>
		<if test="renewalId != null and renewalId != ''">
			and RENEWAL_ID = #{renewalId,jdbcType=VARCHAR}
		</if>
		<if test="insuraCode != null and insuraCode != ''">
			and INSURA_CODE = #{insuraCode,jdbcType=VARCHAR}
		</if>
		<if test="alarmName != null and alarmName != ''">
			and ALARM_NAME = #{alarmName,jdbcType=VARCHAR}
		</if>
		<if test="validInd != null and validInd != ''">
			and VALID_IND = #{validInd,jdbcType=CHAR}
		</if>
		<if test="recognizeeName != null and recognizeeName != ''">
			and RECOGNIZEE_NAME = #{recognizeeName,jdbcType=VARCHAR}
		</if>
		<if test="fristRegetDate != null and fristRegetDate != ''">
			and FRIST_REGET_DATE = #{fristRegetDate,jdbcType=VARCHAR}
		</if>
		<if test="createUser != null and createUser != ''">
			and CREATE_USER = #{createUser,jdbcType=VARCHAR}
		</if>
<!-- 		<if test="renewalGroupCode != null and renewalGroupCode != ''">   -->
<!-- 			and exists ( -->
