		</if>
		<if test="salesmanNo != null and salesmanNo != ''">
			and SALESMAN_NO = #{salesmanNo,jdbcType=VARCHAR}
		</if>
		<if test="renewalDate != null and renewalDate != ''">
			and RENEWAL_DATE = #{renewalDate,jdbcType=DATE}
		</if>
		<if test="renewalSalesmanCode != null and renewalSalesmanCode != ''">
			and RENEWAL_SALESMAN_CODE = #{renewalSalesmanCode,jdbcType=VARCHAR}
		</if>
		<if test="lossRatio != null and lossRatio != ''">
